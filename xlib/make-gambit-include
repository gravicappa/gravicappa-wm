#!/bin/sh

name="$1"

gen_exported() {
	for f in `grep -E '^\(%*define' | cut -d\  -f2 | grep -v '(%'`; do
		item=${f#(*}
		item=${item%*)}
		printf "\n    %s" "$item"
	done
}

cat << !
;;;; This file has been autogenerated by $0. 
;;;; Do NOT bother editing it manually.

(##namespace 
  ("$name" `gen_exported`))
!

